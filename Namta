// Namta
#include<stdio.h>
int main()
{
    int i,j,n1,n2;
    printf("Start from: ");
    scanf("%d",&n1);
    printf("End: ");
    scanf("%d",&n2);
    for(i=n1;i<=n2;i++)
    {
        for(j=1;j<=10;j++)
        {
            printf("%d X %d = %d\n\n",i,j,i*j);
        }
        printf("\n\n");
    }
    return 0;
}
